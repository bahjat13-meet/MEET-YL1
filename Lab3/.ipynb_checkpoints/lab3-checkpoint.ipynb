      "import csv\n",
      "\n",
      "f = open('cig.csv', 'rb')\n",
      "reader = csv.reader(f)\n",
      "weight = []\n",
      "vol = []\n",
      "for row in reader:\n",
      "    r = row[0].split()\n",
      "    rr = map(float, r[1:])\n",
      "    weight.append(rr[1])\n",
      "    vol.append(rr[2])\n",
      "\n",
      "print weight\n",
      "print \"                  \"\n",
      "print vol\n",
      "f.close()"
